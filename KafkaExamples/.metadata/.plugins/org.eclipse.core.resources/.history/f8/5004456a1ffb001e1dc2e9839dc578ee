spring:
  application:
    name: kafka-consumer-example
  kafka:
    consumer:
      bootstrap-servers:
      - localhost:9092
      group-id: sb-test-group-131
      key-deserializer:
        org.apache.kafka.common.serialization.StringSerializer
      value-deserializer:
        org.springframework.kafka.support.serializer.JsonSerializer

server:
  port: 9292